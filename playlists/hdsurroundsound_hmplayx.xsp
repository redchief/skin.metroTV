<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>HD Sound</name>
    <match>all</match>
    <rule field="videoresolution" operator="greaterthan">
        <value>700</value>
    </rule>
    <rule field="audiochannels" operator="greaterthan">
        <value>7</value>
    </rule>
    <order direction="ascending">title</order>
</smartplaylist>
