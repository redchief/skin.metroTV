<?xml version="1.0" encoding="UTF-8"?>

<window id="145">
	<defaultcontrol always="true">11</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>

		<include condition="Window.IsVisible(osdvideosettings) | Window.IsVisible(osdaudiosettings)">DialogOverlay</include>
	
		<control type="group">
			<posx>425</posx>
			<animation type="WindowOpen">
				<effect type="zoom" start="108" end="100" time="400" center="640,360" tween="cubic" easing="out" />
				<effect type="fade" start="0" end="100" time="400" />
			</animation>
			<animation type="WindowClose">
				<effect type="zoom" start="100" end="108" time="400"  center="640,360" tween="cubic" easing="out" />
				<effect type="fade" start="100" end="0" time="400" />
			</animation>
			<control type="image">
				<posx>-20</posx>
				<posy>-20</posy>
				<width>460</width>
				<height>90</height>
				<include>Common_Color_Keyback1</include>
			</control>

			<control type="label" id="10">
				<description>Heading</description>
				<posx>10</posx>
				<width>250</width>
				<label>-</label>
				<font>font22L</font>
				<textcolor>Accent</textcolor>
			</control>

			<control type="label" id="12">
				<description>Label</description>
				<posx>410</posx>
				<width>250</width>
				<label>-</label>
				<font>font22L</font>
				<align>right</align>
			</control>

			<control type="slider" id="11">
				<description>Slider</description>
				<posx>10</posx>
				<posy>40</posy>
				<width>400</width>
				<onleft>11</onleft>
				<onright>11</onright>
			</control>

		</control>

	</controls>

</window>